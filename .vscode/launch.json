{
    "version": "0.2.0",
    "projectName": "NRS-Flight-Computer-Verification",
    "configurations": [
        {
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceRoot}/build/NRS_FC-Verification.elf",
            "name": "Launch with PyOCD",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "showDevDebugOutput": "none",
            "servertype": "pyocd",
            "preLaunchTask": "build", // configured in tasks.json
        },
        {
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceRoot}/build/NRS_FC-Verification.elf",
            "name": "Attach with PyOCD",
            "request": "attach",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "showDevDebugOutput": "none",
            "servertype": "pyocd",
            "preLaunchTask": "build", // configured in tasks.json
        },
        {
            "name": "STlink launch",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/build/NRS_FC-Verification.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "stlink",
            "device": "STM32F407VE",
            "interface": "swd",
            "preLaunchTask": "build", // configured in tasks.json
            // "preLaunchCommands": ["Build all"], // you can execute command instead of task
            "svdFile": "", // Include svd to watch device peripherals
            "swoConfig": {} // currently (v1.7.0) not supported
        },
        {
            "name": "STlink attach",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/build/NRS_FC-Verification.elf",
            "request": "attach",
            "type": "cortex-debug",
            "servertype": "openocd",
            "device": "STM32F407VE",
            "interface": "swd",
            "preLaunchTask": "build", // configured in tasks.json
            // "preLaunchCommands": ["Build all"], // you can execute command instead of task
            "svdFile": "", // Include svd to watch device peripherals
            "swoConfig": {} // currently (v1.7.0) not supported
        },
    ]
}